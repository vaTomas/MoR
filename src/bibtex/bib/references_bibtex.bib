	year = {2021},
	pmid = {33581412},
	keywords = {Damage detection, EMAT, Guided wave, Piezoelectric transducers, SH wave, Structural health monitoring},
	pages = {106355},
}

@article{azimi_structural_2020,
	title = {Structural health monitoring using extremely compressed data through deep learning},
