	pmcid = {PMC9371249},
	keywords = {alkaline to binder ratio, casting, compressive strength, geopolymer concrete},
	pages = {3132},
	file = {Full Text:C\:\\Users\\vince\\Zotero\\storage\\5NXMC663\\Shilar et al. - 2022 - Assessment of Destructive and Nondestructive Analy.pdf:application/pdf},
}

@article{skentou_closed-form_2023,
